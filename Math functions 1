#include <stdio.h>
#include <math.h>

int main() {
    float value;
    int squareValue, base, power;

    
    scanf("%f", &value);
    scanf("%d", &squareValue);
    scanf("%d", &base);
    scanf("%d", &power);

    
    int floorVal = (int)floor(value);
    int ceilVal = (int)ceil(value);
    int sqrtVal = (int)sqrt(squareValue);
    int powVal = (int)pow(base, power);

    
    printf("%d\n", floorVal);
    printf("%d\n", ceilVal);
    printf("%d\n", sqrtVal);
    printf("%d\n", powVal);

    return 0;
}
